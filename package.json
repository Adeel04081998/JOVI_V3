{
  "name": "jovi",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "npx react-native run-android",
    "android-dev-release": "npx react-native run-android --variant=release",
    "ios": "npx react-native run-ios",
    "android-ios": "npx react-native run-android && npx react-native run-ios",
    "start": "react-native start",
    "dev_ios": "cd ios && pod install && cd .. && react-native run-ios",
    "dev_android": "cd android&&./gradlew clean&&./gradlew --stop&&cd - &&npx react-native run-android",
    "ipod": "npx react-native run-ios --simulator \"iPod touch (7th generation)\"",
    "branch_change_new_packages": "yarn && cd android && ./gradlew clean && ./gradlew --stop && cd .. &&  npx react-native run-android && cd ios && pod install && cd .. && npx react-native run-ios",
    "build_android": "cd android&&./gradlew clean&&./gradlew --stop&& ./gradlew bundleRelease &&cd - &&npx react-native run-android --variant=release",
    "qa_codepush_ios_v2_staging": "appcenter codepush release-react -a cibaksolutionspk-gmail.com/JOVI-IOS-V3 -d Staging",
    "qa_codepush_ios_v2_production": "appcenter codepush release-react -a cibaksolutionspk-gmail.com/JOVI-IOS-V3 -d Production",
    "_comment": "[qa_codepush_ios_v2_staging, qa_codepush_ios_v2_production] As we have updated the app name from v3 to v2 so don't be confused with `cibaksolutionspk-gmail.com/JOVI-IOS-V3` in ios v2 commands",
    "qa_codepush_android_v2_staging": "appcenter codepush release-react -a cibaksolutionspk-gmail.com/JOVI-ANDROID-V2-QA -d Staging",
    "qa_codepush_android_v2_production": "appcenter codepush release-react -a cibaksolutionspk-gmail.com/JOVI-ANDROID-V2-QA -d Production",
    "live_codepush_android_v3_staging": "appcenter codepush release-react -a cibaksolutionspk-gmail.com/JOVI-ANDROID-V3 -d Staging",
    "live_codepush_android_v3_production": "appcenter codepush release-react -a cibaksolutionspk-gmail.com/JOVI-ANDROID-V3 -d Production",
    "set:env:debug": "node ./scripts/set-env.js --env debug",
    "set:env:staging": "node ./scripts/set-env.js --env staging",
    "set:env:release": "node ./scripts/set-env.js --env release",
    "run:android:debug": "yarn set:env:debug && yarn android --variant debug",
    "run:android:staging": "yarn set:env:staging && yarn android --variant release",
    "run:android:release": "yarn set:env:release && yarn android --variant release",
    "_comments": "we have added --variant release for both staging and release because we will use staging as a release for QA and actual release for Production",
    "run:ios:debug": "yarn set:env:debug && yarn ios --configuration Debug",
    "run:ios:staging": "yarn set:env:staging && yarn ios --configuration  Release",
    "run:ios:release": "yarn set:env:release && yarn ios --configuration Release",
    "iphone_13_pro_max": "npx react-native run-ios --simulator \"iPhone 13 Pro Max\"",
    "test": "jest",
    "lint": "eslint ."
  },
